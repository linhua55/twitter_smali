<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape android:shape="0">
            <solid android:color="0x7f120084" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="0">
            <solid android:color="0x7f120001" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="0">
            <gradient android:startColor="0x7f120159" android:endColor="0x7f12003a" android:angle="-90.0" android:type="0" android:centerX="0.0" android:centerY="0.0" />
            <stroke android:width="1dp" android:color="0x7f12003a" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</selector>
