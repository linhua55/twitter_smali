<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_checked="true">
        <shape android:shape="0">
            <solid android:color="0x7f120159" />
            <padding android:left="5dp" android:top="4dp" android:right="5dp" android:bottom="4dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="0">
            <solid android:color="0x7f120085" />
            <padding android:left="5dp" android:top="4dp" android:right="5dp" android:bottom="4dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="0">
            <solid android:color="0x106000d" />
            <stroke android:width="1dp" android:color="0x7f12015d" />
            <padding android:left="5dp" android:top="4dp" android:right="5dp" android:bottom="4dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</selector>
